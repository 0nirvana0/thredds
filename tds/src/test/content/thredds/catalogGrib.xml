<?xml version="1.0" encoding="UTF-8"?>
<catalog name="Unit Test Server: see thredds\tds\src\test\java\thredds\tds\TestTdsLocal.java"
         xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
         xmlns:xlink="http://www.w3.org/1999/xlink">

  <service name="latest" serviceType="Resolver" base=""/>

  <service name="all" base="" serviceType="compound">
    <service name="odap" serviceType="OpenDAP" base="/thredds/dodsC/"/>
    <service name="http" serviceType="HTTPServer" base="/thredds/fileServer/"/>
    <service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/"/>
    <service name="cdmremote" serviceType="CdmRemote" base="/thredds/cdmremote/"/>
  </service>

  <!-- test GRIB collection  -->
  <featureCollection featureType="GRIB" name="GFS_CONUS_80km" path="gribCollection/GFS_CONUS_80km">
    <dataFormat>GRIB-1</dataFormat>
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataType>Grid</dataType>

      <documentation type="summary">NCEP GFS Model : AWIPS 202 (I) Grid. National - CONUS (polar stereographic).
        Model runs are made at 0, 6, 12, and 18Z, with analysis and forecasts every 6 hours out 5 days.
        Horizontal = 65 by 43 points, resolution 190.5 km, Polar Stereographic projection.
        Vertical = 1000 to 100 hPa mandatory pressure levels (10 levels); surface, height above ground, pressure
        layers.
      </documentation>
    </metadata>

    <collection spec="${cdmUnitTest}/ncss/GFS/CONUS_80km/GFS_CONUS_80km_#yyyyMMdd_HHmm#\.grib1$" />
    <update startup="true" trigger="allow"/>
    <tdm startup="true" rescan="0 0/5 * * * ? *" trigger="allow"/>
    <gribConfig datasetTypes="Best LatestFile Files" />

  </featureCollection>

    <featureCollection name="RTMA-CONUS_2p5km" featureType="GRIB" harvest="true" path="grib/NCEP/RTMA/CONUS_2p5km">
      <metadata inherited="true">
        <dataFormat>GRIB-2</dataFormat>
        <serviceName>all</serviceName>
        <dataType>Grid</dataType>

        <documentation type="summary">
          NCEP Model horizontal = 1377 by 2145 points, resolution 2.5 km, LambertConformal projection.
          Model runs are made hourly, with 0 hour forecast
          The NCEP RTMA Model is Real Time Mesoscale Analysis models.
        </documentation>
        <documentation xlink:href="http://www.weather.gov/infoservicechanges/tin08-56_rtma.txt"
                       xlink:title="RTMA TIN"/>
        <documentation xlink:href="http://www.coaps.fsu.edu/~dvandyke/wrfarw/rtma.html"
                       xlink:title="COAPS FSU"/>
      </metadata>

      <collection
          spec="/data/ldm/pub/native/grid/NCEP/RTMA/CONUS_2p5km/.*grib2$"
          dateFormatMark="#RTMA_CONUS_2p5km_#yyyyMMdd_HHmm"
          olderThan="5 min"/>
      <update startup="nocheck" trigger="allow"/>
      <tdm startup="true" rescan="0 2,17,32,47 * * * ? *" trigger="allow"/>

      <gribConfig datasetTypes="Best LatestFile Files">
        <pdsHash>
          <useGenType>true</useGenType>
        </pdsHash>
        <latestNamer name="Latest Real Time Mesoscale Analysis CONUS 2.5km"/>
        <bestNamer name="Best Real Time Mesoscale Analysis CONUS 2.5km Time Series"/>
        <filesSort>
          <lexigraphicByName increasing="false"/>
        </filesSort>
      </gribConfig>
    </featureCollection>



</catalog>
